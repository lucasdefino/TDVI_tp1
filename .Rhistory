train <- titanic_train
test <- titanic_test
str(train)
train$Pclass <- as.factor(train$Pclass)
train$Sex <- as.factor(train$Sex)
table(is.na(train))
summary(train)
set.seed(22)
tree <- rpart(formula = Survived ~ Pclass + Sex + Age + SibSp + Parch + Fare + Embarked,
data = train,
method = "class")
